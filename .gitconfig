[user]
    name = richard maisano
    email = rickmaisano@gmail.com

[color]
    ui = auto
  [color "branch"]
    current = yellow
    remote = green
  [color "diff"]
    meta = yellow
    frag = magenta
    old = red
    new = green
    whitespace = red reverse
  [color "status"]
    added = green
    changed = yellow
    untracked = cyan
  [core]
    whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol

[alias]
    st = status -sb
    co = checkout
    b = branch
    ds = diff --staged
    rekt = reset --hard
    prom = pull --rebase origin master
